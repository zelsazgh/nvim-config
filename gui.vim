filetype plugin on
colorscheme dracula
set relativenumber
set background=dark
let mapleader=","
set nu
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4

set incsearch
set mouse=a
set expandtab
set ambiwidth=double
set helplang=cn
set encoding=utf-8
set fencs=utf-8,ucs-bom.gb18030,gbk,gb2312,cp936
set termencoding=utf-8
set fileencoding=utf-8
set fileencodings=ucs-bom,utf-8,cp936

